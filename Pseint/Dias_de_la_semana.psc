Proceso Dias_de_la_semana
	Definir dia Como entero;
	Escribir "Escribe un número de las semana" Sin Saltar;
	Leer dia;
	Segun dia Hacer
		1:
			Escribir "Lunes";
		2:
			Escribir "Martes";
		3:
			Escribir "Miercoles";
		4:
			Escribir "Jueves";
		5:
			Escribir "Viernes";
		6:
			Escribir "Sabado";
		7:
			Escribir "Domingo";
		De Otro Modo:
			Escribir "No has introducido un día válido";
	FinSegun
	
FinProceso
